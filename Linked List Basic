#include <stdio.h>

struct student{
    int roll;
    char name[20] ;
    int marks;
    struct student* next;
    
}n1,n2,n3;

int  main()
{
    struct student* p;
    scanf("%d %s %d",&n1.roll,n1.name,&n1.marks);
    scanf("%d %s %d",&n2.roll,n2.name,&n2.marks);
    scanf("%d %s %d",&n3.roll,n3.name,&n3.marks);
    
    n1.next=&n2;
    n2.next=&n3;
    n3.next=NULL;
    
    p=&n1;
    
    while(p!=NULL)
    {
        printf("%d %s %d",p->roll,p->name,p->marks);
        p=p->next;
    }
     return 0;   
}
    
    
